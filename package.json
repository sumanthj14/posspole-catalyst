{
  "name": "posspole-catalyst",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:prod": "cross-env NODE_ENV=production next build",
    "build:analyze": "cross-env ANALYZE=true npm run build",
    "predeploy": "npm run build:prod && npm run verify:build",
    "deploy": "node deploy.js",
    "deploy:manual": "touch out/.nojekyll && gh-pages -d out --dotfiles",
    "clean": "rimraf .next out",
    "verify": "npm run lint && npm run build:prod",
    "verify:build": "node verify-build.js"
  },
  "dependencies": {
    "@headlessui/react": "^2.1.9",
    "@heroicons/react": "^2.1.5",
    "@types/node": "22.7.4",
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.20",
    "eslint": "8.40.0",
    "eslint-config-next": "14.2.14",
    "next": "^14.2.14",
    "postcss": "8.4.47",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-slick": "^0.30.2",
    "slick-carousel": "^1.8.1",
    "tailwindcss": "3.4.13",
    "typescript": "5.6.2"
  },
  "devDependencies": {
    "@types/react-slick": "^0.23.13",
    "gh-pages": "^6.3.0",
    "cross-env": "^7.0.3",
    "rimraf": "^5.0.5"
  }
}
